{
    "ci": {
        "publicData": {
            "reference_genomes_ci": {
                "name": "NCBI RefSeq Genomes",
                "source": "refseq",
                "sourceLabel": "RefSeq",
                "type": "KBaseGenomes.Genome",
                "sourceType": "search",
                "templates": {
                    "title": "{{scientific_name}}",
                    "metadata": [
                        {
                            "label": "Lineage",
                            "template": "{{lineage metadata.Taxonomy}}"
                        },
                        {
                            "label": "KBase ID",
                            "template": "{{genome_id}}"
                        },
                        {
                            "label": "RefSeq ID",
                            "template": "{{genome_source_id}}"
                        },
                        {
                            "label": "",
                            "template": [
                                {
                                    "label": "Contigs",
                                    "template": "{{numeral num_contigs \"0,0\"}}"
                                },
                                {
                                    "label": "Features",
                                    "template": "{{numeral num_features \"0,0\"}}"
                                }
                            ]
                        }
                    ]
                },
                "searchFields": null
            },
            "fungal_genomes": {
                "name": "MycoCosm Genomes",
                "type": "KBaseGenomes.Genome",
                "workspaceName": "Fungal_Genomes",
                "lineageStart": "Fungi",
                "iconUrl": "/narrative/static/kbase/images/logos/jgi-mycocosm-logo.png",
                "templates": {
                    "title": "{{metadata.Name}}",
                    "metadata": [
                        {
                            "label": "Taxonomy",
                            "template": "{{lineage metadata.Taxonomy 'Fungi'}}"
                        },
                        {
                            "label": "MycoCosm ID",
                            "template": "{{objectName}}"
                        },
                        {
                            "label": "",
                            "template": [
                                {
                                    "label": "Contigs",
                                    "template": "{{numeral metadata.[Number contigs] \"0,0\"}}"
                                },
                                {
                                    "label": "Features",
                                    "template": "{{numeral metadata.[Number of CDS] \"0,0\"}}"
                                },
                                {
                                    "label": "GC",
                                    "template": "{{numeral metadata.[GC content] \"0.0%\"}}"
                                }
                            ]
                        }
                    ]
                },
                "searchFields": [
                    "metadata.Name", "metadata.Domain", "objectName", "metadata.Taxonomy"
                ],
                "sort": [
                    {
                        "field": "metadata.Name",
                        "ascending": true
                    }
                ],
                "sourceType": "workspace"
            },
